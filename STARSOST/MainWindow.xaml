<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:STARSOST"
        xmlns:UIElements="clr-namespace:STARSOST.UIElements" x:Class="STARSOST.MainWindow"
        mc:Ignorable="d"
        Title="STARS Offline Support Tools" Height="600" Width="800" WindowState="Maximized" WindowStartupLocation="CenterScreen">
    <Window.Background>
        <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.AppWorkspaceColorKey}}"/>
    </Window.Background>
    <Grid>
        <UIElements:MainDashboard HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</Window>
